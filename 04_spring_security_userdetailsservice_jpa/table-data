
 CREATE TABLE SPRING_SECUTIRY_USER (
    id int NOT NULL AUTO_INCREMENT,
    USER_NAME varchar(255) NOT NULL,    
    PASS varchar(255),
    ROLES varchar(255),
    ACTVE boolean,
    PRIMARY KEY (id)
);

